{
  "rules": [
    {
      "validation": "camelCase",
      "patterns": ["**/*"]
    },
    {
      "validation": "PascalCase",
      "patterns": [
        "src/app/components/**",
        "src/app/pages/**",
        "src/app/hocs/**"
      ]
    },
    {
      "validation": "ignore",
      "patterns": [
        "**/typings/**/*",
        "**/__tests__/**/*",
        "**/__mocks__/**/*",
        "**/__snapshots__/**/*",
        "**/docker-compose.yml",
        "**/Dockerfile*",
        "**/LICENSE",
        "**/README.md",
        "src/app/assets/algolia/*",
        "_*.scss",
        "c__*"
      ]
    }
  ]
}
