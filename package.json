{
  "name": "architecture",
  "version": "1.0.0",
  "description": "The architectures of Hollowverse",
  "private": true,
  "main": "index.js",
  "repository": "https://github.com/hollowverse/architecture.git",
  "scripts": {
    "test": "npm-run-all --aggregate-output -p -c common-check lint",
    "lint": "run-p -c --aggregate-output lint-css lint-js lint-ts check-ts",
    "lint-css": "stylelint --syntax scss 'src/app/**/*.scss'",
    "lint-js": "eslint '**/*.js{,x}'",
    "lint-ts/eslint": "eslint 'src/app/**/*.ts{,x}' --config src/app/.eslintrc.json --no-eslintrc",
    "lint-ts/tslint/app": "tslint 'src/app/**/*.ts{,x}' --project src/app",
    "lint-ts/tslint/root": "tslint '**/*.ts{,x}' --project . --exclude 'node_modules/**/*' --exclude 'src/app/**/*'",
    "lint-ts/tslint": "run-p -c --aggregate-output 'lint-ts/tslint/*'",
    "lint-ts": "run-p -c --aggregate-output 'lint-ts/*'",
    "app/check-ts": "tsc --project ./src/app --noEmit",
    "check-ts": "run-p -c --aggregate-output '*/check-ts'",
    "app/webpack/build": "ES5=1 NODE_ENV=production NO_PERF_CHECKS=1 webpack --config ./src/webpack/webpack.config.js",
    "app/build": "run-s app/webpack/build",
    "server/app-server/dev": "APP_SERVER_PORT=${APP_SERVER_PORT:-3001} HOT=1 NODE_ENV=development webpack-dev-server --config ./src/webpack/webpack.config.js --display-error-details --hot",
    "build": "npm-run-all -s clean app/build",
    "dev": "NODE_ENV=development npm-run-all --aggregate-output -s clean -p server/app-server/dev",
    "clean/build": "del-cli './dist'",
    "clean": "run-p 'clean/*'",
    "validate-filenames": "validate-filenames",
    "lint-staged": "lint-staged",
    "deploy": "babel-node ./deploy.js",
    "pre-push": "yarn test",
    "pre-commit": "yarn common-check",
    "common-check": "run-p -l -c --aggregate-output validate-filenames 'clown check' check-integrity",
    "clown": "clown",
    "check-integrity": "yarn check --integrity"
  },
  "license": "Unlicense",
  "homepage": "https://github.com/hollowverse/architecture#readme",
  "lint-staged": {
    "**/*.md": [
      "prettier --write",
      "git add"
    ],
    "**/*.{j,t}s{x,}": [
      "prettier --write",
      "git add"
    ],
    "**/!(package).json": [
      "prettier --write",
      "git add"
    ],
    "**/*.js{x,}": [
      "eslint"
    ],
    "**/**/*.{s,}css": [
      "stylelint --syntax scss",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p -l -c --aggregate-output lint-staged pre-commit",
      "pre-push": "yarn pre-push",
      "post-merge": "yarnhook",
      "post-checkout": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  },
  "browserslist": [
    "and_chr >= 62",
    "chrome >= 55",
    "edge >= 14",
    "firefox >= 57",
    "ios_saf >= 9.3",
    "safari >= 9.1"
  ],
  "resolutions": {
    "@types/react": "^16.0.35",
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.44",
    "@babel/runtime": "^7.0.0-beta.44",
    "bluebird": "^3.5.1",
    "classcat": "^1.0.1",
    "common-tags": "^1.7.2",
    "domready": "^1.0.8",
    "helmet": "^3.9.0",
    "history": "^4.6.3",
    "lodash": "^4.17.5",
    "lodash-es": "^4.17.8",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet-async": "^0.0.4",
    "react-hot-loader": "^4.1.1",
    "react-loadable": "^5.3.1",
    "react-remarkable": "^1.1.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^5.0.0",
    "vis": "^4.21.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
